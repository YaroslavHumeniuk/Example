{
      "compilerOptions": {
              "declaration": false
            , "emitDecoratorMetadata": false
            , "experimentalDecorators": false
            , "inlineSourceMap": false
            , "inlineSources": false
            , "isolatedModules": false
            , "mapRoot": ""
            , "module": "amd"
            , "newLine": "CRLF"
            , "noEmit": false
            , "noEmitHelpers": true
            , "noImplicitAny": true
            , "noResolve": true
            , "outFile": "final.js"
            , "preserveConstEnums": false
            , "removeComments": false
            , "sourceMap": false
            , "sourceRoot": ""
            , "suppressImplicitAnyIndexErrors": false
            , "target": "es3"
      }
}